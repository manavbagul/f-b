{
  "version": 2,
  "builds": [{ "src": "api/index.vercel.js", "use": "@vercel/node" }],
  "routes": [
    { "src": "/api/a", "dest": "/api/index.vercel.js" },
    { "src": "/api/(.*)", "dest": "/api/application.js" },
    { "src": "/", "dest": "/api/test.js" },
    { "src": "/pdfs/(.*)", "dest": "/api/pdf/$1" }
  ]
}
